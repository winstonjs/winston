    "eslint-config-populist": "^4.1.0",
    "hock": "^1.3.2",
    "mocha": "^3.2.0",
    "nyc": "^11.2.1",
    "split2": "^2.1.1",
    "std-mocks": "^1.0.0",
    "through2": "^2.0.0",
    "winston-compat": "^0.0.1"
