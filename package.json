    "test": "mocha",
    "test:coverage": "nyc npm run test:unit",
    "test:unit": "mocha test/unit",
    "test:integration": "mocha test/integration",
    "build": "rimraf dist && babel lib -d dist",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">= 12.0.0"
